{
    "Defaults": {
    },
    "Deployments": [
        { "input": ".env.mustache", "output": ".env" },
        { "input": ".gitignore.mustache", "output": ".gitignore" },
        { "input": "docker-compose.mustache", "output": "docker-compose.yml" },
        { "input": "nginx.conf.mustache", "output": "nginx.conf" },
        { "input": "policy-relationship-role.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/policy-relationship-role.json" },
        { "input": "policy-assume-role.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/policy-assume-role.json" },
        { "input": "policy-deployment.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/policy-deployment.json" },
        { "input": "policy-functions.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/policy-functions.json" },
        { "input": "policy-execute-api.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/policy-execute-api.json" },
        { "input": "aws-setup.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/aws-setup.sh" },
        { "input": "aws-cleanup.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/aws-cleanup.sh" },
        { "input": "deployment-input.mustache", "output": ".simplify-graphql/{{DeploymentEnvSnake}}/deployment-input.json" },
        { "input": "deployment-setup.mustache", "output": ".simplify-graphql/setup.sh" },
        { "input": "deployment-cleanup.mustache", "output": ".simplify-graphql/cleanup.sh" },
        { "input": "deployment.mustache", "output": ".simplify-graphql/deployment.yaml" },
        { "input": "deployment-push.mustache", "output": ".simplify-graphql/deployment.js" },
        { "input": "deployment-package.mustache", "output": "package.json" },
        { "input": "code-push.mustache", "output": ".simplify-graphql/code-push.sh" },
        { "input": "code-init.mustache", "output": ".simplify-graphql/code-init.js" },
        { "input": "verbal-design.mustache", "output": "verbal-design.txt" },
        { "input": "README.mustache", "output": "README.md" }
    ],
    "GraphQLServers": [
        { "input": ".babelrc.mustache", "output": "{{serverName}}/.babelrc" },
        { "input": "docker-entrypoint.mustache", "output": "{{serverName}}/docker-entrypoint.sh" },
        { "input": "Dockerfile.mustache", "output": "{{serverName}}/Dockerfile" },
        { "input": "server-input.mustache", "output": "{{serverName}}/.simplify-graphql/{{DeploymentEnvSnake}}/server-input.json" },
        { "input": "function-push.mustache", "output": "{{serverName}}/.simplify-graphql/function-push.js" },
        { "input": "layer-push.mustache", "output": "{{serverName}}/.simplify-graphql/layer-push.js" },
        { "input": "webpack.config.mustache", "output": "{{serverName}}/webpack.config.js" },
        { "input": "webpack.config.layer.mustache", "output": "{{serverName}}/webpack.config.layer.js" },
        { "input": "webpack.config.deps.mustache", "output": "{{serverName}}/webpack.config.deps.js" },
        { "input": "package.mustache", "output": "{{serverName}}/package.json" },
        { "input": "hashbag.mustache", "output": "{{serverName}}/loaders/hashbag.js" },
        { "input": "package-src.mustache", "output": "{{serverName}}/src/package.json" },
        { "input": "package-deps.mustache", "output": "{{serverName}}/deps/nodejs/node_modules/{{NameSnake}}-deps/package.json" },
        { "input": "logger.mustache", "output": "{{serverName}}/src/Utilities/logger.js" },
        { "input": "iso8601.mustache", "output": "{{serverName}}/src/Utilities/iso8601.js" },
        { "input": "api-utils.mustache", "output": "{{serverName}}/src/Utilities/api-utils.js" },
        { "input": "sigv4gw.mustache", "output": "{{serverName}}/src/Utilities/sigv4gw.js" },
        { "input": "schema.mustache", "output": "{{serverName}}/src/schema.graphql" },
        { "input": "index.mustache", "output": "{{serverName}}/src/index.js" },
        { "input": "deps.mustache", "output": "{{serverName}}/src/deps.js" },
        { "input": "function-index.mustache", "output": "{{serverName}}/src/Functions/functions.js" },
        { "input": "app.mustache", "output": "{{serverName}}/src/app.js" },
        { "input": "app.env.mustache", "output": "{{serverName}}/src/.env" },
        { "input": "model/.babelrc.mustache", "output": "{{serverName}}/models/.babelrc" },
        { "input": "model/package.mustache", "output": "{{serverName}}/models/package.json" },
        { "input": "model/dynamodb.mustache", "output": "{{serverName}}/models/src/dynamodb.js" },
        { "input": "model/cassandra.mustache", "output": "{{serverName}}/models/src/cassandra.js" },
        { "input": "model/index.mustache", "output": "{{serverName}}/models/src/index.js" }
    ],
    "GraphQLDataInputs": [
        { "input": "input.mustache", "output": "{{serverName}}/src/Schemas/Inputs/{{dataName}}.js" }
    ],
    "GraphQLDataObjects": [
        { "input": "model.mustache", "output": "{{serverName}}/src/Schemas/Models/{{dataName}}.js" }
    ],
    "GraphQLEnumObjects": [
        { "input": "enum.mustache", "output": "{{serverName}}/src/Schemas/Models/{{dataName}}.js" }
    ],
    "GraphQLResolvers": [
        { "input": "resolver.mustache", "output": "{{serverName}}/src/Resolvers/{{stateName}}.js" },
        { "input": "tests.spec.mustache", "output": "{{serverName}}/tests/samples/{{stateName}}.spec.js" }
    ],
    "GraphQLFunctions": [
        { "input": "function.mustache", "output": "{{serverName}}/src/Functions/{{functionName}}{{#RemoteExecutionMode}}/index{{/RemoteExecutionMode}}.js" }
    ],
    "Executions": [
        { "input": "lambda/deps.mustache", "output": "Executions/{{functionName}}/src/deps.js" },
        { "input": "lambda/debug.mustache", "output": "Executions/{{functionName}}/src/debug.js" },
        { "input": "lambda/handler.mustache", "output": "Executions/{{functionName}}/src/index.js" },
        { "input": "lambda/index-src.mustache", "output": "Executions/{{functionName}}/src/{{functionName}}.js"},
        { "input": "lambda/package-src.mustache", "output": "Executions/{{functionName}}/src/package.json" },
        { "input": "function-push.mustache", "output": "Executions/{{functionName}}/.simplify-graphql/function-push.js" },
        { "input": "lambda/package-deps.mustache", "output": "Executions/{{functionName}}/deps/nodejs/node_modules/{{functionNameSnake}}-deps/package.json" },
        { "input": "lambda/function-input.mustache", "output": "Executions/{{functionName}}/.simplify-graphql/{{DeploymentEnvSnake}}/function-input.json" },
        { "input": "layer-push.mustache", "output": "Executions/{{functionName}}/.simplify-graphql/layer-push.js" },
        { "input": "hashbag.mustache", "output": "Executions/{{functionName}}/loaders/hashbag.js" },
        { "input": "lambda/sonar-project.mustache", "output": "Executions/{{functionName}}/.simplify-graphql/sonar-project.properties" },
        { "input": "lambda/sonarqube-scanner.mustache", "output": "Executions/{{functionName}}/.simplify-graphql/sonarqube-scanner.sh" },
        { "input": "lambda/webpack.config.mustache", "output": "Executions/{{functionName}}/webpack.config.js" },
        { "input": "lambda/webpack.config.deps.mustache", "output": "Executions/{{functionName}}/webpack.config.deps.js" },
        { "input": "lambda/webpack.config.layer.mustache", "output": "Executions/{{functionName}}/webpack.config.layer.js" },
        { "input": "lambda/.babelrc.mustache", "output": "Executions/{{functionName}}/.babelrc" },
        { "input": "lambda/package.mustache", "output": "Executions/{{functionName}}/package.json" }
    ]
}