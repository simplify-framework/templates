{
    "Defaults": {
    },
    "Deployments": [
        { "input": ".env.mustache", "output": ".env" },
        { "input": ".gitignore.mustache", "output": ".gitignore" },
        { "input": "docker-compose.mustache", "output": "docker-compose.yml" },
        { "input": "nginx.conf.mustache", "output": "nginx.conf" },
        { "input": "policy-relationship-role.mustache", "output": ".simplify-graphql/policy-relationship-role.json" },
        { "input": "policy-assume-role.mustache", "output": ".simplify-graphql/policy-assume-role.json" },
        { "input": "policy-deployment.mustache", "output": ".simplify-graphql/policy-deployment.json" },
        { "input": "policy-functions.mustache", "output": ".simplify-graphql/policy-functions.json" },
        { "input": "policy-execute-api.mustache", "output": ".simplify-graphql/policy-execute-api.json" },
        { "input": "deployment.mustache", "output": ".simplify-graphql/deployment.yaml" },
        { "input": "deployment-push.mustache", "output": ".simplify-graphql/deployment.js" },
        { "input": "deployment-input.mustache", "output": ".simplify-graphql/deployment-input.json" },
        { "input": "deployment-package.mustache", "output": "package.json" },
        { "input": "code-push.mustache", "output": ".simplify-graphql/code-push.sh" },
        { "input": "code-init.mustache", "output": ".simplify-graphql/code-init.js" },
        { "input": "aws-setup.mustache", "output": ".simplify-graphql/aws-setup.sh" },
        { "input": "aws-cleanup.mustache", "output": ".simplify-graphql/aws-cleanup.sh" },
        { "input": "verbal-design.mustache", "output": "verbal-design.txt" }
    ],
    "GraphQLServers": [
        { "input": ".babelrc.mustache", "output": "{{serverName}}/.babelrc" },
        { "input": "docker-entrypoint.mustache", "output": "{{serverName}}/docker-entrypoint.sh" },
        { "input": "Dockerfile.mustache", "output": "{{serverName}}/Dockerfile" },
        { "input": "server-input.mustache", "output": "{{serverName}}/.simplify-graphql/server-input.json" },
        { "input": "function-push.mustache", "output": "{{serverName}}/.simplify-graphql/function-push.js" },
        { "input": "layer-push.mustache", "output": "{{serverName}}/.simplify-graphql/layer-push.js" },
        { "input": "webpack.config.mustache", "output": "{{serverName}}/webpack.config.js" },
        { "input": "webpack.config.layer.mustache", "output": "{{serverName}}/webpack.config.layer.js" },
        { "input": "package.mustache", "output": "{{serverName}}/package.json" },
        { "input": "package-src.mustache", "output": "{{serverName}}/src/package.json" },
        { "input": "logger.mustache", "output": "{{serverName}}/src/Utilities/logger.js" },
        { "input": "api-utils.mustache", "output": "{{serverName}}/src/Utilities/api-utils.js" },
        { "input": "sigV4Client.mustache", "output": "{{serverName}}/src/Utilities/sigV4Client.js" },
        { "input": "schema.mustache", "output": "{{serverName}}/src/schema.graphql" },
        { "input": "index.mustache", "output": "{{serverName}}/src/index.js" },
        { "input": "function-index.mustache", "output": "{{serverName}}/src/Functions/functions.js" },
        { "input": "app.mustache", "output": "{{serverName}}/src/app.js" }
    ],
    "GraphQLDataInputs": [
        { "input": "input.mustache", "output": "{{serverName}}/src/Schemas/Inputs/{{dataName}}.js" }
    ],
    "GraphQLDataObjects": [
        { "input": "model.mustache", "output": "{{serverName}}/src/Schemas/Models/{{dataName}}.js" }
    ],
    "GraphQLEnumObjects": [
        { "input": "enum.mustache", "output": "{{serverName}}/src/Schemas/Models/{{dataName}}.js" }
    ],
    "GraphQLResolvers": [
        { "input": "resolver.mustache", "output": "{{serverName}}/src/Resolvers/{{stateName}}.js" }
    ],
    "GraphQLFunctions": [
        { "input": "function.mustache", "output": "{{serverName}}/src/Functions/{{functionName}}.js" }
    ],
    "LambdaFunctions": [    
        { "input": "lambda/handler.mustache", "output": "LambdaFunctions/{{functionName}}/src/index.js" },
        { "input": "lambda/index-src.mustache", "output": "LambdaFunctions/{{functionName}}/src/{{functionNameSnake}}/index.js"},
        { "input": "lambda/package-src.mustache", "output": "LambdaFunctions/{{functionName}}/src/package.json" },
        { "input": "function-push.mustache", "output": "LambdaFunctions/{{functionName}}/.simplify-graphql/function-push.js" },
        { "input": "lambda/function-input.mustache", "output": "LambdaFunctions/{{functionName}}/.simplify-graphql/function-input.json" },
        { "input": "layer-push.mustache", "output": "LambdaFunctions/{{functionName}}/.simplify-graphql/layer-push.js" },
        { "input": "lambda/sonar-project.mustache", "output": "LambdaFunctions/{{functionName}}/.simplify-graphql/sonar-project.properties" },
        { "input": "lambda/sonarqube-scanner.mustache", "output": "LambdaFunctions/{{functionName}}/.simplify-graphql/sonarqube-scanner.sh" },
        { "input": "lambda/webpack.config.mustache", "output": "LambdaFunctions/{{functionName}}/webpack.config.js" },
        { "input": "lambda/webpack.config.layer.mustache", "output": "LambdaFunctions/{{functionName}}/webpack.config.layer.js" },
        { "input": "lambda/.babelrc.mustache", "output": "LambdaFunctions/{{functionName}}/.babelrc" },
        { "input": "lambda/package.mustache", "output": "LambdaFunctions/{{functionName}}/package.json" }
    ]
}