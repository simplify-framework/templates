const router = require('./utils/router');
exports.handler = (event, context) => {
    router.register(event, context, [
        {{#serviceEntries}}
        {{#operations}}
        { method: "{{httpMethod}}", path: "{{replacedPathName}}", handler: require("./operations/{{operationId}}") },
        {{/operations}}
        {{/serviceEntries}}
    ]);
};