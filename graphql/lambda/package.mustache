{
    "name": "{{FunctionNameSnake}}",
    "version": "{{#Version}}{{Version}}{{/Version}}{{^Version}}0.1.0{{/Version}}",
    "description": "Remote Function Execution For {{#Servers}}{{ServerName}}{{#hasMore}}, {{/hasMore}}{{/Servers}}",
    "main": "app.js",
    "scripts": {
        "build-code": "npm run build-deps && cd src/ && npm link {{FunctionNameSnake}}-deps && rm -rf node_modules",
        "build-code-minify": "webpack -p --config webpack.config.js",
        "production": "webpack -p && env LOCAL_SERVER= node build/index.js",
        "development": "webpack --mode development && env LOCAL_SERVER=YES node -r dotenv/config build/index.js",
        "start": "cd src && npm install && npm link {{FunctionNameSnake}}-deps && env LOCAL_SERVER=YES AWS_XRAY_CONTEXT_MISSING=LOG_ERROR node -r dotenv/config debug.js dotenv_config_path=../.env"
    },
    "license": "MIT",
    "private": true,
    "dependencies": {
        "debug": "^4.1.1"
    },
    "devDependencies": {
        "dotenv": "8.2.0",
        "simplify-sdk": "^0.1.49",
        "aws-sdk": "^2.713.0",
        "adm-zip": "^0.4.16",
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.14.0",
        "@babel/node": "^7.13.13",
        "@babel/preset-env": "^7.14.0",
        "@babel/register": "^7.13.16",
        "@babel/plugin-transform-runtime": "^7.13.15",
        "license-checker-webpack-plugin": "^0.2.1",
        "babel-loader": "^8.2.2",
        "file-loader": "^6.0.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-node-externals": "^2.3.0"
    }
