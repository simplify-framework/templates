{
    "Profile": "{{DeploymentProfile}}",
    "Region": "{{DeploymentRegion}}",
    "AccountId": "{{DeploymentAccount}}",
    "Environment": "{{DeploymentEnvSnake}}",
    "Bucket": {
        "Name": "{{DeploymentBucket}}-{{ProjectId}}-{{DeploymentEnvSnake}}",
        "Key": "stacks/${DATE_TODAY}"
    },
    "OutputFile": "{{DeploymentEnvSnake}}/{{ProjectName}}-meta.json",
    "Deployment": {
        "Name": "{{ProjectName}}{{DeploymentEnvPascal}}",
        "Definition": "deployment.yaml",
        "ApiAuthorizerId": "{{ApiAuthorizerId}}",
        "Servers": {
            {{#Servers}}
            "{{Name}}": {
                "FunctionInput": "../{{Name}}/.simplify-graphql/{{DeploymentEnvSnake}}/server-input.json",
                "ManagedResourceArn": "NO",
                "SecretKeyVault": "NO"
            }{{#hasMore}}, {{/hasMore}}
            {{/Servers}}
        },
        "Functions": {
            {{#Functions}}
            "{{FunctionName}}": {
                "DependsOn": [{{#Servers}}"{{ServerName}}"{{#hasMore}}, {{/hasMore}}{{/Servers}}],
                "FunctionInput": "../Executions/{{FunctionName}}/.simplify-graphql/{{DeploymentEnvSnake}}/function-input.json",
                "ManagedResourceArn": "NO",
                "SecretKeyVault": "NO"
            }{{#hasMore}}, {{/hasMore}}
            {{/Functions}}
        }
    }
}